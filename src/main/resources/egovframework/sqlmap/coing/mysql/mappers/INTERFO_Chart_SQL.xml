<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="egovframework.coing.chart.service.impl.ChartMapper">
  
  <select id="exchangeRecyclingChart" resultType="map">
    CALL SP_GET_EMD_RESOURCE_STATS(#{year}, ${month});
  </select>
  
  <select id="pointChart" resultType="map">
    CALL SP_GET_MONTHLY_POINT_STATS(#{year});
  </select>
  
  <select id="recyclingMonthlyChart" resultType="map">
    CALL SP_GET_WEEKLY_RESOURCE_STATS(#{month}, #{exchange});
  </select>
  
  <select id="recyclingResourcesChart" resultType="map">
    CALL SP_GET_RESOURCE_COLLECTION_STATS(#{year}, #{month});
  </select>

</mapper>